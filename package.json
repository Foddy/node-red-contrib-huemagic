{
  "name": "node-red-contrib-huemagic",
  "version": "2.0.2",
  "description": "Philips Hue node to control bridges, lights, groups, scenes, rules, taps, switches, motion sensors, temperature sensors and Lux sensors using Node-RED.",
  "author": "foddy",
  "homepage": "https://github.com/Foddy/node-red-contrib-huemagic",
  "license": "Apache-2.0",
  "maintainers": [
    "Foddy Botosakis <github@foddys.com>"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Foddy/node-red-contrib-huemagic.git"
  },
  "bugs": {
    "url": "https://github.com/Foddy/node-red-contrib-huemagic/issues"
  },
  "keywords": [
    "node-red",
    "node-red-contrib",
    "contrib",
    "philips",
    "hue",
    "api",
    "iot"
  ],
  "node-red": {
    "nodes": {
      "hue-bridge": "red/hue-bridge-config.js",
      "hue-bridge-node": "red/hue-bridge.js",
      "hue-light": "red/hue-light.js",
      "hue-group": "red/hue-group.js",
      "hue-scene": "red/hue-scene.js",
      "hue-tap": "red/hue-tap.js",
      "hue-switch": "red/hue-switch.js",
      "hue-motion": "red/hue-motion.js",
      "hue-temperature": "red/hue-temperature.js",
      "hue-brightness": "red/hue-brightness.js",
      "hue-rules": "red/hue-rules.js"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "hex-rgb": "^4.1.0",
    "huejay": "^1.9.0",
    "moment": "^2.24.0",
    "rgb-hex": "^3.0.0",
    "request": "^2.88.0",
    "colornames": "^1.1.1",
    "color-namer": "^1.3.0",
    "get-image-colors": "^2.0.0"
  }
}
